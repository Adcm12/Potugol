// Desenvolva um programa que leia dois números inteiros e troque seus valores sem usar uma variável auxiliar.
// Em seguida, exiba os valores atualizados.

programa {

  funcao inicio() {

    inteiro num1, num2

    escreva("Digite o primeiro número: ")
    leia(num2) 

    escreva("Digite o segundo número: ")
    leia(num1) 

    escreva("\nPrimeiro número: ", num1) 
    escreva("\nSegundo número: ", num2)

  }
}
